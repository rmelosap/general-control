generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model User {
  id                String   @id @default(uuid()) @db.Uuid

  pst_grd           String   @db.VarChar(2)
  pst_grd_completo  String   @db.VarChar(5)

  quadro            String
  especialidade     String
  funcao            String
  operacionalidade  String
  dependente        String

  genero            String

  email_funcional   String
  email_particular  String

  trigrama          String   @db.VarChar(3)

  nome_de_guerra    String
  nome_completo     String

  saram             String   @db.VarChar(7)
  cpf               String   @db.VarChar(14)

  banco             String   @db.VarChar(3)
  agencia           String   @db.VarChar(10)
  conta_corrente    String
}
